{
  "outputDirectory": "public",
  "functions": {
    "api/**/*.rs": {
      "runtime": "vercel-rust@4.0.6"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "http://localhost:3000,https://localhost:3000,localhost:3000"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "OPTIONS, POST, GET, PUT, DELETE, PATCH"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, x-requested-with, x-auth-token, origin, accept"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ]
}
